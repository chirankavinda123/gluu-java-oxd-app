<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.1" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_3_1.xsd">
    <servlet>
        <servlet-name>MainServlet</servlet-name>
        <servlet-class>com.wijesekara.testapp.MainServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginServlet</servlet-name>
        <servlet-class>com.wijesekara.testapp.loginServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WelcomeServlet</servlet-name>
        <servlet-class>com.wijesekara.testapp.WelcomeServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginCeDev</servlet-name>
        <servlet-class>com.wijesekara.testapp.loginCeDev</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>loginIs</servlet-name>
        <servlet-class>com.wijesekara.testapp.loginIs</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>oauth2client</servlet-name>
        <servlet-class>com.wijesekara.testapp.oauth2client</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>WelcomeServeletIS</servlet-name>
        <servlet-class>com.wijesekara.testapp.WelcomeServeletIS</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>MainServlet</servlet-name>
        <url-pattern>/main</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginServlet</servlet-name>
        <url-pattern>/login</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WelcomeServlet</servlet-name>
        <url-pattern>/welcome</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginCeDev</servlet-name>
        <url-pattern>/loginCeDev</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>loginIs</servlet-name>
        <url-pattern>/loginIs</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>oauth2client</servlet-name>
        <url-pattern>/oauth2client</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>WelcomeServeletIS</servlet-name>
        <url-pattern>/WelcomeServeletIS</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
